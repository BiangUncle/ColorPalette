.head_container {
  width: 750rpx;
}
.head_container .block {
  margin: 30rpx 30rpx 60rpx 30rpx;
  flex-direction: column;
}
.head_container .block .head {
  position: absolute;
}
.head_container .block .head .head_list {
  width: 60rpx;
  height: 60rpx;
  margin: 40rpx;
}
.head_container .block .head .Color {
  font-size: 80rpx;
  margin: 60rpx 10rpx -15rpx 30rpx;
  color: white;
  font-family: "Arial Rounded MT";
  font-weight: bold;
}
.head_container .block .head .sfc {
  font-size: 40rpx;
  margin: 0 0 0 30rpx;
  color: white;
  font-family: "Calibri", sans-serif;
}
.head_container .block .background {
  width: 100%;
  height: 500rpx;
  border-radius: 30rpx;
  box-shadow: 15rpx 15rpx 20rpx 5rpx rgba(92, 92, 92, 0.15);
}
.list_container {
  width: 100%;
  align-items: center;
  display: flex;
  flex-direction: column;
}
.list_container .itemcontainer {
  width: 690rpx;
  box-shadow: 15rpx 15rpx 20rpx 5rpx rgba(92, 92, 92, 0.15);
  margin-bottom: 50rpx;
  border-radius: 30rpx;
  position: relative;
}
.list_container .itemcontainer .imagecontainer {
  position: relative;
}
.list_container .itemcontainer .imagecontainer image {
  width: 690rpx;
  border-radius: 30rpx;
}
.list_container .itemcontainer .imagecontainer .imagetitle {
  font-size: 40rpx;
  line-height: 60rpx;
  padding: 0 20rpx;
  color: rgba(0, 0, 0, 0.5);
  margin: 20rpx 0;
}
.list_container .itemcontainer .detailcontainer {
  width: 690rpx;
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  height: 450.26rpx;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 40rpx;
  line-height: 40rpx;
  padding-top: 50rpx;
}
.list_container .itemcontainer .detailcontainer .detailbar {
  display: flex;
  margin-top: 30rpx;
  color: white;
  align-items: center;
}
.list_container .itemcontainer .detailcontainer .detailbar .barcontainer {
  width: 400rpx;
  height: 30rpx;
  border-radius: 15rpx;
  background-color: rgba(178, 178, 178, 0.5);
  margin-left: 10rpx;
}
.list_container .itemcontainer .detailcontainer .lang {
  margin: 50rpx 0 20rpx 0;
  height: 50rpx;
  width: 150rpx;
  border-radius: 25rpx;
  background-color: #242424;
  color: white;
  text-align: center;
  font-size: 30rpx;
  line-height: 50rpx;
}
.list_container .itemcontainer .detailcontainer .tags {
  font-size: 25rpx;
  color: rgba(255, 255, 255, 0.5);
}
.changeType {
  position: absolute;
  top: 470rpx;
  z-index: 5;
  display: flex;
  justify-content: center;
  width: 100%;
}
.changeType .changeTypecontainer {
  width: 400rpx;
  background-color: white;
  display: flex;
  box-shadow: 5rpx 5rpx 20rpx 10rpx rgba(0, 0, 0, 0.05);
  border-radius: 25rpx;
  letter-spacing: 5px;
  align-items: center;
  font-weight: bold;
  color: rgba(0, 0, 0, 0.5);
}
.changeType .changeTypecontainer .typetitle {
  width: 400rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.changeType .changeTypecontainer .typeList {
  height: 270rpx;
  width: 400rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow-y: scroll;
}
.changeType .changeTypecontainer .typeList p {
  line-height: 90rpx;
}
.copybutton{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
}